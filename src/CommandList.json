{
  "help": {
    "command": "--help",
    "short": "-h",
    "description": "Display all control commands in the console"
  },
  "init": {
    "command": "--init",
    "description": "Initial React project, required argument '--name'",
    "required": {
      "name": {
        "command": "--name",
        "short": "-n",
        "type": "string"
      }
    }
  },
  "generate": {
    "command": "--generate",
    "short": "-g",
    "description": "Generate component. Required arguments for type generate entity (component, service, page). Example --component ComponentName",
    "args": {
      "component": {
        "command": "--component",
        "short": "-c",
        "type": "string"
      },
      "service": {
        "command": "--service",
        "short": "-s",
        "type": "string"
      },
      "page": {
        "command": "--page",
        "short": "-p",
        "type": "string"
      }
    }
  }
}
